/* This sets the color for the layouts icon show on the
home page */
.n-hopin-styleguide-c-layouts-card--left, .n-hopin-styleguide-c-layouts-card--right, .n-hopin-styleguide-c-layouts-card--bottom {
  stroke: var(--hopin-home-item-outline, #454545);
}

.n-hopin-styleguide-c-layouts-card--left {
  fill: var(--hopin-home-item-color-2, #00aeef);
}

.n-hopin-styleguide-c-layouts-card--right {
  fill: var(--hopin-home-item-color-3, #ec008c);
}

.n-hopin-styleguide-c-layouts-card--bottom {
  fill: var(--hopin-home-item-color-1, #fff200);
}
